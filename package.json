{
  "name": "template-ts-vite-lit-tailwind",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "cover": "vitest run --coverage",
    "e2e:open": "cypress open --e2e",
    "e2e:run": "cypress run --e2e",
    "e2e:verify": "cypress verify",
    "e2e:start:build": "yarn build && concurrently --raw --kill-others --success \"all\" \"yarn preview\" \"yarn e2e:open --config baseUrl=http://localhost:4173/\"",
    "e2e:start:dev": "concurrently --raw --kill-others --success \"all\" \"yarn dev\" \"yarn e2e:open\"",
    "analyze": "lit-analyzer --strict src/**/*.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier -w --list-different .",
    "lint:staged": "lint-staged",
    "prepare": "husky install && yarn e2e:verify"
  },
  "dependencies": {
    "lit": "^2.2.7"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.15",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "concurrently": "^7.3.0",
    "cssnano": "^5.1.12",
    "cypress": "^10.3.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lit-analyzer": "1.2.1",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "tailwindcss": "^3.1.6",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.2",
    "vitest": "^0.18.1"
  }
}
